= simple_form_for(@event) do |f|
  = f.error_notification

  .form-inputs
    = f.input :start_date
    = f.input :end_date
    / = f.input :status
    / = f.input :client_id
    = f.association :car
    = f.input :service_tokens, data: { load: @event.service_types }
    / = f.input :created_by

  .form-actions
    = f.button :submit
